package com.app.entity;

import jakarta.persistence.*;
import lombok.*;

@Entity(name = "Issues")
@Data
@Getter
@Setter
@NoArgsConstructor
public class Issue {
	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private int issueId;
	
	@Column(name = "ProjectName")
	private String issueName;
	
	@Column
	private int projectId;
}
